{
    "env": {
        "OPENROUTER_API_KEY": "${OPENROUTER_API_KEY}",
        "TELEGRAM_BOT_TOKEN": "${TELEGRAM_BOT_TOKEN}"
    },
    "agents": {
        "defaults": {
            "models": {
                "openrouter/anthropic/claude-sonnet-4": {
                    "alias": "sonnet"
                },
                "openrouter/google/gemini-2.0-flash-001": {
                    "alias": "flash"
                },
                "openrouter/openai/gpt-4o": {
                    "alias": "gpt4o"
                }
            },
            "model": {
                "primary": "openrouter/anthropic/claude-sonnet-4",
                "fallbacks": [
                    "openrouter/google/gemini-2.0-flash-001",
                    "openrouter/openai/gpt-4o"
                ]
            },
            "workspace": "/root/.openclaw/workspace",
            "sandbox": {
                "mode": "off"
            },
            "thinkingDefault": "low",
            "elevatedDefault": "on",
            "timeoutSeconds": 1800,
            "subagents": {
                "model": "openrouter/google/gemini-2.0-flash-001",
                "maxConcurrent": 2
            }
        },
        "list": [
            {
                "id": "master",
                "default": true,
                "identity": {
                    "name": "Teddy Power",
                    "theme": "AI 商业伙伴 / 规划与执行方",
                    "emoji": ""
                }
            }
        ]
    },
    "gateway": {
        "bind": "lan",
        "port": 8080,
        "controlUi": {
            "allowInsecureAuth": true
        },
        "auth": {
            "mode": "token"
        }
    },
    "channels": {
        "telegram": {
            "enabled": true,
            "botToken": "${TELEGRAM_BOT_TOKEN}",
            "dmPolicy": "allowlist",
            "allowFrom": [
                "tg:7322349199"
            ],
            "groups": {
                "*": {
                    "requireMention": true
                }
            }
        }
    },
    "tools": {
        "profile": "full",
        "elevated": {
            "enabled": true,
            "allowFrom": {
                "telegram": [
                    "tg:7322349199"
                ]
            }
        },
        "web": {
            "search": {
                "enabled": true
            },
            "fetch": {
                "enabled": true
            }
        }
    },
    "commands": {
        "native": "auto",
        "text": true,
        "bash": true,
        "config": true
    },
    "logging": {
        "level": "info"
    }
}